{
  "name": "okx-kline-predictor",
  "version": "1.0.0",
  "description": "OKX K-line data fetcher and predictor using TensorFlow.js",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "train": "node examples/train_and_predict.js",
    "predict": "node -e \"require('./src').predict().then(console.log)\"",
    "update": "node -e \"require('./src').updateAndPredict().then(console.log)\"",
    "test:connection": "node examples/test_connection.js",
    "init": "cp .env.example .env",
    "data:update": "node examples/update_data.js",
    "data:status": "node -e \"require('./examples/update_data')().catch(console.error)\""
  },
  "keywords": [
    "okx",
    "tensorflow",
    "machine-learning",
    "trading",
    "cryptocurrency"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.22.0",
    "axios": "^1.6.7",
    "ccxt": "^4.4.29",
    "dotenv": "^16.4.1",
    "https-proxy-agent": "^7.0.5",
    "moment": "^2.30.1",
    "node-cache": "^5.1.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
